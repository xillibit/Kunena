{
  "name": "kunena/kunena-forum",
  "description": "Kunena forum for joomla",
  "keywords": ["kunena", "forum"],
  "homepage": "https://github.com/Kunena/Kunena-Forum/",
  "license": "GPL-3.0-or-later",
  "support": {
        "issues": "https://github.com/Kunena/Kunena-Forum/issues",
        "forum": "https://www.kunena.org/forum/index",
        "docs": "https://www.kunena.org/docs/en"
  },
  "require": {
    "php": ">=7.2.0",
    "consolidation/robo": "^1",
    "symfony/console": "~3|~4",
    "symfony/filesystem": "~3|~4",
    "symfony/finder": "~3|~4",
    "symfony/process": "~3|~4",
    "symfony/yaml": "~3|~4",
    "ext-simplexml": "*",
    "ext-dom": "*",
    "ext-json": "*",
    "leafo/lessphp": "~0.5.0",
        "vanilla/nbbc": "~2.2.3",
        "scssphp/scssphp": "~1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "joomla/cms-coding-standards": "~2.0.0-alpha2@dev",
    "joomla/coding-standards": "~3.0@dev",
    "friendsofphp/php-cs-fixer": "~2.12",
    "squizlabs/php_codesniffer": "~3.0",
    "joomla-projects/joomla-browser": "~4.0@dev",
    "codeception/codeception": "~3.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
    "phing/phing": "2.*",
    "kunena/codestyle": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Codeception\\Module\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
